              item.hide();
          }
          }
          
        
        
        $(function() {
          //$('#feedback_link').click(function(e){
          //  e.preventDefault();
          //  $('#feedback_form').show();
          //});
          clicked = true;
          clicked2 = true;
          $('#name').focus(function(){
            if(clicked){
            $('#name').val('');
            clicked = false;
            }
          })
          $('#gen_com').focus(function(){
            if(clicked2){
            $('#gen_com').val('');
            clicked2 = false;
            }
          })
          
          $("#comments_response_required_true").click(function(){
            update_comments();
            });
          $("#comments_response_required_false").click(function(){
            update_comments();
            });
           update_comments();
      
          
          $("#send_com_form").click(function(e) {
            e.preventDefault()
            var url = window.location.href;
            var comment = $('#gen_com').val();
            $('#gen_com').val(comment+' -- submitted from URL:'+url);
            var email = $('#name').val();
            var met_id = $('#met_id').val();
            var resp = $('#comments_response_required_false').val();
            var form_vars = $("#feedback").serialize(); 
            //alert(form_vars);
            if(comment==''||email==''||met_id==''){
              alert('nothing typed : form not sending!!');
              //maybe do something else if this is tripped
            }
            else {
              var dataString = 'name='+ email + '&comment=' + comment + '&metadata_id=' + met_id + '&response_required=' + resp;
              
              $.ajax({
                type: "POST",
                url: "http://data-search.nerc.ac.uk/comments/",
                data: form_vars,
                success: function() {
                  //alert('message accepted by comments application');
                  $('#feedback_form_resp').html('<p style="color:green">Your feedback has been received, if requested you shall be contacted shortly</p>');
                  $('#feedback_form').hide();
                  $('#feedback_form_resp').show('slow');
                  
                },
                error: function() {
                  $('#feedback_form_resp').html('<p style="color:red">There was an error sending your feedback, our engineers have been notified</p>');
                }
              });

            }
          });
        });

